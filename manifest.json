{
  "manifest_version": 2,

  "name": "URL Shortener",
  "description": "Shorten the current tab URL with Google URL Shortener",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "version": "1.1.1",
  "author": "Varun Dey",
   "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
